<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #434242;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uap_pbo.ProdukInputController">
   <children>
      <VBox alignment="CENTER" layoutY="12.0" prefHeight="377.0" prefWidth="600.0" spacing="100.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Label alignment="CENTER" text="Tambah Produk" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="73.0" prefWidth="600.0" spacing="150.0">
               <children>
                  <Button fx:id="btnMakanan" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#barangIpt" prefHeight="50.0" prefWidth="90.0" style="-fx-background-color: #22A39F;" text="Barang" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnProduk" mnemonicParsing="false" onAction="#makanInp" prefHeight="50.0" style="-fx-background-color: #22A39F;" text="Makanan" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <Button fx:id="btnKembali" alignment="CENTER" mnemonicParsing="false" onAction="#kembali" style="-fx-background-color: #22A39F;" text="Kembali" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
