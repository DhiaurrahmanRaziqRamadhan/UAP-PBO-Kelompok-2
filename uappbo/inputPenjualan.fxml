<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="479.0" prefWidth="673.0" style="-fx-background-color: #434242;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uap_pbo.InputPenjualanController">
   <children>
      <VBox alignment="CENTER" layoutX="110.0" layoutY="20.0" prefWidth="454.0" spacing="80.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Label alignment="CENTER" text="Masukkan Data Penjualan" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox spacing="100.0">
               <children>
                  <VBox prefHeight="220.0" prefWidth="103.0" spacing="50.0">
                     <children>
                        <Label text="Nama Produk" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font></Label>
                        <Label text="Jumlah Produk" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font></Label>
                        <Label text="Stok" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font></Label>
                        <Label text="Harga" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font></Label>
                     </children>
                  </VBox>
                  <VBox spacing="40.0">
                     <children>
                        <TextField fx:id="Dnama" prefHeight="25.0" prefWidth="275.0" promptText="Nama Data" style="-fx-background-color: #5c5b5b;" />
                        <TextField fx:id="Djumlah" prefHeight="25.0" prefWidth="275.0" promptText="Jumlah Produk" style="-fx-background-color: #5c5b5b;" />
                        <TextField fx:id="Dstok" prefHeight="25.0" prefWidth="275.0" promptText="Stok" style="-fx-background-color: #5c5b5b;" />
                        <TextField fx:id="Dharga" prefHeight="25.0" prefWidth="275.0" promptText="Harga" style="-fx-background-color: #5c5b5b;" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" spacing="50.0">
               <children>
                  <Button fx:id="btnKembali" mnemonicParsing="false" onAction="#kembali" style="-fx-background-color: #22A39F;" text="Kembali" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnTambah" mnemonicParsing="false" onAction="#tambah" style="-fx-background-color: #22A39F;" text="Tambah" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnLihat" mnemonicParsing="false" onAction="#lihat" style="-fx-background-color: #22A39F;" text="Lihat Data" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
